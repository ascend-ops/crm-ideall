{
	"name": "supastarter-nextjs",
	"private": true,
	"scripts": {
		"build": "dotenv -c -- turbo build",
		"dev": "dotenv -c -- turbo dev --concurrency 15",
		"type-check": "dotenv -c -- turbo type-check",
		"start": "dotenv -c -- turbo start",
		"lint": "biome lint . ",
		"check": "biome check",
		"clean": "turbo clean",
		"format": "biome format . --write"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "pnpm@10.14.0",
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@repo/tsconfig": "workspace:*",
		"@types/node": "^24.10.1",
		"dotenv-cli": "^11.0.0",
		"turbo": "^2.6.1",
		"typescript": "5.9.3"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.2.2",
			"@types/react-dom": "19.2.2",
			"trim-newlines": ">=3.0.1",
			"glob": ">=11.1.0",
			"qs": ">=6.14.1",
			"fast-xml-parser": ">=5.3.4",
			"@isaacs/brace-expansion": ">=5.0.1",
			"lodash": ">=4.17.23",
			"js-yaml": ">=4.1.1",
			"mdast-util-to-hast": ">=13.2.1",
			"@smithy/config-resolver": ">=4.4.0",
			"esbuild": ">=0.25.0",
			"body-parser": ">=2.2.1"
		},
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@swc/core",
			"esbuild",
			"prisma",
			"prisma-zod-generator",
			"sharp"
		]
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.84.0",
		"@tanstack/react-query-devtools": "^5.91.1",
		"flag": "^5.0.1",
		"recharts": "^3.5.0"
	}
}
